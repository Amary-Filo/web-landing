        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^20.2.11",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.5",
    "@floating-ui/dom": "^1.7.4",
    "@ng-icons/core": "^32.2.0",
    "countries-and-timezones": "^3.8.0",
    "express": "^5.1.0",
    "ng-primitives": "^0.86.0",
    "ngx-device-detector": "^10.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "ua-parser-js": "^2.0.6"
